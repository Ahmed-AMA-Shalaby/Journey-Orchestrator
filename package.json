{
  "name": "journey-orchestrator",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format:fix": "prettier --write 'src/**/*.{ts,tsx,css}'",
    "lint": "eslint src/**/*.{ts,tsx} --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest --run --passWithNoTests",
    "test:staged": "vitest related --run --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.4.2",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "@mui/x-date-pickers": "^7.6.1",
    "@reduxjs/toolkit": "^2.2.5",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/react-redux": "^7.1.33",
    "dayjs": "^1.11.11",
    "jsdom": "^24.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "vitest": "^1.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.13.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vitest": "^0.4.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
